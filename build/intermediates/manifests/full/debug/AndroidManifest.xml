<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="leeon.mobile.BBSBrowser"
    android:versionCode="211"
    android:versionName="2.1.1" >

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="leeon.mobile.BBSBrowser.BrowserActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.BoardTabActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/welcome_label" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.BoardListActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.DocListActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.ContentListActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity android:name="leeon.mobile.BBSBrowser.ContentDrawingActivity" >
        </activity>
        <activity android:name="leeon.mobile.BBSBrowser.SettingActivity" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.MailDetailActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.ContentDetailActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.SearchDocListActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.PathListActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.PathContentActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
        <activity android:name="leeon.mobile.BBSBrowser.ImageDetailActivity" >
        </activity>
        <activity android:name="leeon.mobile.BBSBrowser.GIFViewActivity" >
        </activity>
        <activity
            android:name="leeon.mobile.BBSBrowser.OtherBoardTabActivity"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
    </application>

</manifest>