<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="leeon.mobile.BBSBrowser"
      android:versionCode="211"
      android:versionName="2.1.1">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".BrowserActivity"
        	android:label="@string/app_name" android:configChanges="orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".BoardTabActivity" 
        	android:label="@string/welcome_label" android:configChanges="orientation|keyboardHidden">
        </activity>
        
        <activity android:name=".BoardListActivity" android:configChanges="orientation|keyboardHidden">
        </activity>
        
        <activity android:name=".DocListActivity" android:configChanges="orientation|keyboardHidden">
        </activity>
        
        <activity android:name=".ContentListActivity" android:configChanges="orientation|keyboardHidden">
        </activity>
        
        <activity android:name=".ContentDrawingActivity">
        </activity>
        
        <activity android:name=".SettingActivity">
        </activity>
        
        <activity android:name=".MailDetailActivity" android:configChanges="orientation|keyboardHidden">        	
        </activity>
        
        <activity android:name=".ContentDetailActivity" android:configChanges="orientation|keyboardHidden">        	
        </activity>
        
        <activity android:name=".SearchDocListActivity" android:configChanges="orientation|keyboardHidden">        	
        </activity>
        
        <activity android:name=".PathListActivity" android:configChanges="orientation|keyboardHidden">
        </activity>
        
        <activity android:name=".PathContentActivity" android:configChanges="orientation|keyboardHidden">
        </activity>

        <activity android:name=".ImageDetailActivity">
        </activity>
        
        <activity android:name=".GIFViewActivity">
        </activity>
        
        <activity android:name=".OtherBoardTabActivity" android:configChanges="orientation|keyboardHidden">
        </activity>
    </application>
    <supports-screens android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:anyDensity="true"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest> 